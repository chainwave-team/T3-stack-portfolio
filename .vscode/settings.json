{
  "cSpell.words": [
    "Fira",
    "Scrollable",
    "camelcase",
    "prismic",
    "reactjs",
    "rebass",
    "uniqid"
  ],
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "files.exclude": {
    "**/.cache": true,
    "**/generators": true,
    "**/public": true,
    "**/data": true,
    "**/static": true,
    "**/custom_types": true,
    "**/*.d.ts": true,
    "**/.idea": true,
    "**/.vscode": true,
    "**/.DS_store": true,
    "**/logs": true,
    "**/*.log": true,
    "**/npm-debug.log*": true,
    "**/yarn-debug.log*": true,
    "**/yarn-error.log*": true,
    "**/pids": true,
    "**/*.pid": true,
    "**/*.seed": true,
    "**/*.pid.lock": true,
    "**/lib-cov": true,
    "**/coverage": true,
    "**/.nyc_output": true,
    "**/.grunt": true,
    "**/bower_components": true,
    "**/.lock-wscript": true,
    "build/Release": true,
    "**/node_modules/": true,
    "**/jspm_packages/": true,
    "**/typings/": true,
    "**/.npm": true,
    "**/.eslintcache": true,
    "**/.node_repl_history": true,
    "**/*.tgz": true,
    "**/.env": true,
    "**/.cache/": true,
    "**/.DS_Store": true,
    "**/yarn-error.log": true,
    "**/.pnp/": true,
    "**/.pnp.js": true,
    "**/.": true
  }
}