{
  "Main": {
    "title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1",
        "label": "Title"
      }
    },
    "introduction": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, preformatted, heading1, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink",
        "allowTargetBlank": true,
        "label": "Introduction"
      }
    },
    "header_contact_button_label": {
      "type": "Text",
      "config": {
        "label": "Header Contact Button Label",
        "placeholder": "Contactez moi"
      }
    }
  },
  "SEO": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "Unique ID",
        "placeholder": "Type an SEO-friendly identifier..."
      }
    },
    "meta_title": {
      "type": "Text",
      "config": {
        "label": "Meta Title",
        "placeholder": "Meta title..."
      }
    },
    "meta_description": {
      "type": "Text",
      "config": {
        "label": "Meta Description",
        "placeholder": "Meta Description..."
      }
    },
    "canonical": {
      "type": "Link",
      "config": {
        "select": "document",
        "customtypes": [
          "brewery",
          "country"
        ],
        "label": "Canonical URL",
        "placeholder": "Select the category to use in the canonical URL"
      }
    }
  },
  "Services": {
    "services_introduction": {
      "type": "StructuredText",
      "config": {
        "single": "paragraph",
        "allowTargetBlank": true,
        "label": "Services introduction"
      }
    },
    "services": {
      "type": "Group",
      "config": {
        "fields": {
          "status": {
            "type": "Select",
            "config": {
              "options": [
                "default",
                "new",
                "soon",
                "hide"
              ],
              "default_value": "default",
              "label": "status"
            }
          },
          "service_title": {
            "type": "Text",
            "config": {
              "label": "Service title"
            }
          },
          "service_textarea": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph, preformatted, heading1, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink, image, embed, list-item, o-list-item, o-list-item",
              "allowTargetBlank": true,
              "label": "Service Textarea"
            }
          }
        },
        "label": "Services"
      }
    }
  }
}